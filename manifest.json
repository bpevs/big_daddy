{
  "manifest_version": 2,

  "name": "Big Daddy",
  "description": "Big Daddy finds any mention of the word \"data\" and replaces it with the superior word \"daddy\".",
  "version": "1.0",

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["replace.js"],
      "run_at": "document_end"
    }
  ]
}
